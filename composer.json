{
  "name": "teamelf/core",
  "description": "Extensible team management",
  "keywords": ["team", "management"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Zhihao Jiang",
      "email": "guessever@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "TeamELF\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "monolog/monolog": "^1.23",
    "symfony/routing": "^4.0",
    "symfony/finder": "^4.0",
    "symfony/filesystem": "^4.0",
    "symfony/http-foundation": "^4.0",
    "symfony/event-dispatcher": "^4.0",
    "pascaldevink/shortuuid": "^2.0",
    "vlucas/phpdotenv": "^2.4",
    "doctrine/orm": "^2.5",
    "symfony/validator": "^4.0",
    "twig/twig": "^2.4",
    "doctrine/migrations": "^1.6",
    "overtrue/pinyin": "~3.0",
    "swiftmailer/swiftmailer": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "mockery/mockery": "^1.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
